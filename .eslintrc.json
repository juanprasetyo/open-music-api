{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "max-len": 0,
        "newline-per-chained-call": 0
    }
}
